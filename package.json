{
  "name": "test-template1",
  "version": "1.0.0",
  "description": "A template for my test projects",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild" : "rm -rf ./docs",
    "build": "parcel build --out-dir docs --public-url ./ ./src/index.html",
    "start": "parcel serve ./src/index.html --open",
    "lint" : "eslint ./src",
    "lint-fix": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnnaHarshyna/test-template1.git"
  },
  "keywords": [
    "template",
    "boilerplate",
    "html",
    "scss",
    "javascript",
    "parcel",
    "eslint"
  ],
